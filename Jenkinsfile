pipeline {
    agent any
    stages {
        stage ("CD") {
            steps {
            sh 'cd /home/nikita'
            }
        }
            stage ("pwd") {
                steps {
                sh 'pwd'
            }
        }
        
           stage ("ls -la") {
               steps {
               sh 'ls -la'
            }
        }
        
           stage ("cd ~") {
               steps {
               sh 'cd ~'
            }
        }
        
           stage ("instal mc") {
               steps {
               sh 'sudo apt-get install mc'
            }
        }
           stage ("instal tree") {
                steps {
                sh 'sudo apt-get install mc'
            }
        }
            stage ("instal mc") {
               steps {
               sh 'sudo apt-get install java'
            }
        }
     }
 }


