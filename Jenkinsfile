pipeline {
    agent any
    stages {
        stage ("CD") {
            steps {
            sh 'cd /home/nikita'
            }
        }
            stage ("pwd") {
                steps {
                sh 'pwd'
            }
        }
        
           stage ("ls -la") {
               steps {
               sh 'ls -la'
            }
        }
     }
 }


